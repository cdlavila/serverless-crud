{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "User",
    "type": "object",
    "required": ["full_name", "email"],
    "properties": {
      "full_name": {
        "type": "string",
        "description": "The person's full name.",
        "default": ""
      },
      "email": {
        "description": "Email address of the user.",
        "type": "string",
        "default": ""
      }
    }
}